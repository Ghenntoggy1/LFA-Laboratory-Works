// Graphical Representation of Finite Automaton
digraph {
	node [shape=circle]
	S
	node [shape=circle]
	B
	node [shape=circle]
	D
	node [shape=doublecircle]
	q_f
	S -> B [label=a]
	S -> B [label=b]
	B -> D [label=b]
	B -> B [label=c]
	B -> S [label=a]
	D -> q_f [label=b]
	D -> D [label=a]
	node [shape=none]
	start [label=""]
	start -> S
}
