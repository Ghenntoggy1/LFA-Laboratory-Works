// Graphical Representation of Finite Automaton
digraph {
	node [shape=circle]
	q0
	node [shape=circle]
	q0q1
	node [shape=doublecircle]
	q0q1q2
	node [shape=doublecircle]
	q0q2
	q0 -> q0q1 [label=a]
	q0 -> q0 [label=b]
	q0q1 -> q0q1q2 [label=a]
	q0q1 -> q0 [label=b]
	q0q1q2 -> q0q1q2 [label=a]
	q0q1q2 -> q0q2 [label=b]
	q0q2 -> q0q1q2 [label=a]
	q0q2 -> q0q2 [label=b]
	node [shape=none]
	start [label=""]
	start -> q0
}
