// Graphical Representation of Finite Automaton
digraph {
	node [shape=circle]
	S
	node [shape=circle]
	A
	node [shape=circle]
	B
	node [shape=doublecircle]
	C
	node [shape=doublecircle]
	SC
	S -> A [label=a]
	S -> B [label=b]
	A -> C [label=a]
	A -> B [label=b]
	B -> SC [label=c]
	SC -> A [label=a]
	SC -> B [label=b]
	node [shape=none]
	start [label=""]
	start -> S
}
