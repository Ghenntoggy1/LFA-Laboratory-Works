// Graphical Representation of Finite Automaton
digraph {
	node [shape=circle]
	S
	node [shape=circle]
	A
	node [shape=circle]
	B
	node [shape=doublecircle]
	C
	node [shape=doublecircle]
	SC
	S -> A [label=a]
	S -> B [label=b]
	S -> q_d [label=c]
	A -> C [label=a]
	A -> B [label=b]
	A -> q_d [label=c]
	B -> q_d [label="a, b"]
	B -> SC [label=c]
	C -> q_d [label="a, b, c"]
	SC -> A [label=a]
	SC -> B [label=b]
	SC -> q_d [label=c]
	node [shape=none]
	start [label=""]
	start -> S
}
