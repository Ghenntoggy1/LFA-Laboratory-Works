// Graphical Representation of Finite Automaton
digraph {
	node [shape=circle]
	q0
	node [shape=circle]
	q1
	node [shape=doublecircle]
	q2
	q0 -> q0 [label=a]
	q0 -> q1 [label=a]
	q0 -> q0 [label=b]
	q1 -> q2 [label=a]
	q2 -> q2 [label=a]
	q2 -> q2 [label=b]
	node [shape=none]
	start [label=""]
	start -> q0
}
